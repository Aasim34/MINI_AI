// This is your Prisma schema file
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id     String
  name     String
  email     String
  password     String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

model Photos {
  id     String
  title     String
  description     String?
  url     String
  category     String
  createdAt     Date
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

model Contacts {
  id     String
  name     String
  email     String
  message     String
  createdAt     Date
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

